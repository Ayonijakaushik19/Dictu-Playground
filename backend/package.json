{
  "name": "dictu-playground-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint --fix",
    "prepare": "cd .. && husky install backend/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd backend && npm run lint -- --fix",
      "commit-msg": "cd backend && npx --no-install commitlint --edit $1"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "eslint": "^7.29.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "husky": "^6.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4"
  }
}
